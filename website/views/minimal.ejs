    <script>
        // Particles
        function createParticles() {
            const container = document.getElementById('particles');
            const colors = ['#FF9900', '#3B82F6', '#8B5CF6'];
            
            for (let i = 0; i < 10; i++) {
                const particle = document.createElement('div');
                particle.className = 'particle';
                particle.style.width = Math.random() * 4 + 2 + 'px';
                particle.style.height = particle.style.width;
                particle.style.left = Math.random() * 100 + '%';
                particle.style.top = Math.random() * 100 + '%';
                particle.style.backgroundColor = colors[Math.floor(Math.random() * colors.length)];
                particle.style.opacity = Math.random() * 0.3 + 0.1;
                particle.style.animation = `float ${Math.random() * 3 + 2}s ease-in-out infinite`;
                particle.style.animationDelay = Math.random() * 2 + 's';
                container.appendChild(particle);
            }
        }

        // Theme toggle
        const toggleBtn = document.getElementById('theme-toggle');
        const sunIcon = document.getElementById('sun-icon');
        const moonIcon = document.getElementById('moon-icon');
        
        function setTheme(isDark) {
            if (isDark) {
                document.documentElement.classList.add('dark');
                localStorage.setItem('theme', 'dark');
            } else {
                document.documentElement.classList.remove('dark');
                localStorage.setItem('theme', 'light');
            }
        }
        
        toggleBtn?.addEventListener('click', () => {
            const isDark = !document.documentElement.classList.contains('dark');
            setTheme(isDark);
        });

        // Load theme
        const savedTheme = localStorage.getItem('theme');
        const prefersDark = window.matchMedia('(prefers-color-scheme: dark)').matches;
        if (savedTheme === 'dark' || (!savedTheme && prefersDark)) {
            setTheme(true);
        }

        // Smooth scroll
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                target?.scrollIntoView({ behavior: 'smooth' });
            });
        });

        // Calculate overall progress and analytics
        function updateOverallProgress() {
            const progress = JSON.parse(localStorage.getItem('examProgress') || '{}');
            const total = 16;
            const completed = Object.values(progress).filter(p => p.completed).length;
            const passed = Object.values(progress).filter(p => p.completed && p.score >= 72).length;
            const percentage = Math.round((completed / total) * 100);
            
            document.getElementById('overall-progress').textContent = percentage + '%';
            document.getElementById('overall-progress-bar').style.width = percentage + '%';
            document.getElementById('completed-count').textContent = completed + ' completed';
            document.getElementById('passed-count').textContent = passed + ' passed';
            
            // Analytics
            const scores = Object.values(progress).filter(p => p.completed).map(p => p.score);
            const avgScore = scores.length > 0 ? Math.round(scores.reduce((a, b) => a + b, 0) / scores.length) : 0;
            const passRate = completed > 0 ? Math.round((passed / completed) * 100) : 0;
            const totalAttempts = Object.values(progress).reduce((sum, p) => sum + (p.attempts || 0), 0);
            
            document.getElementById('avg-score').textContent = avgScore + '%';
            document.getElementById('exams-completed').textContent = completed + '/16';
            document.getElementById('pass-rate').textContent = passRate + '%';
            document.getElementById('total-attempts').textContent = totalAttempts;
        }

        // Show completion badges, progress, and difficulty
        function updateExamCards() {
            const progress = JSON.parse(localStorage.getItem('examProgress') || '{}');
            
            Object.keys(progress).forEach(examId => {
                const data = progress[examId];
                if (data.completed && data.score >= 72) {
                    const badge = document.querySelector(`.completion-badge-${examId}`);
                    if (badge) badge.style.display = 'block';
                }
                
                // Update progress bar
                const progressBar = document.getElementById(`progress-bar-${examId}`);
                if (progressBar && data.completed) {
                    progressBar.style.width = data.score + '%';
                }
                
                // Update score display
                const scoreEl = document.getElementById(`score-${examId}`);
                if (scoreEl && data.completed) {
                    scoreEl.textContent = `Last: ${data.score}% • ${data.passed ? 'Passed ✓' : 'Failed'}`;
                    scoreEl.className = `mt-4 text-xs ${data.passed ? 'text-green-600 dark:text-green-400' : 'text-red-600 dark:text-red-400'}`;
                }
                
                // Update quick stats
                const bestScore = document.querySelector(`.best-score-${examId}`);
                const attempts = document.querySelector(`.attempts-${examId}`);
                if (bestScore) bestScore.textContent = data.score > 0 ? `${data.score}%` : '—';
                if (attempts) attempts.textContent = data.attempts || 0;
            });
            
            // Add difficulty badges
            for (let i = 1; i <= 16; i++) {
                const badge = document.querySelector(`.difficulty-badge-${i}`);
                if (badge) {
                    let difficulty, color;
                    if (i <= 4) { difficulty = 'Easy'; color = 'bg-green-100 text-green-700 dark:bg-green-900/30 dark:text-green-400'; }
                    else if (i <= 8) { difficulty = 'Medium'; color = 'bg-yellow-100 text-yellow-700 dark:bg-yellow-900/30 dark:text-yellow-400'; }
                    else if (i <= 12) { difficulty = 'Hard'; color = 'bg-orange-100 text-orange-700 dark:bg-orange-900/30 dark:text-orange-400'; }
                    else { difficulty = 'Expert'; color = 'bg-red-100 text-red-700 dark:bg-red-900/30 dark:text-red-400'; }
                    
                    badge.textContent = difficulty;
                    badge.className = `difficulty-badge-${i} text-xs px-2 py-1 rounded-full font-medium ${color}`;
                }
            }
        }

        // Clear all progress
        function clearProgress() {
            if (confirm('Are you sure you want to clear all exam progress? This cannot be undone.')) {
                localStorage.removeItem('examProgress');
                for (let i = 1; i <= 16; i++) {
                    localStorage.removeItem(`exam_${i}_state`);
                }
                location.reload();
            }
        }

        // Copy link to clipboard
        function copyLink() {
            navigator.clipboard.writeText('https://aws-exams.matthewntsiful.com').then(() => {
                alert('Link copied to clipboard!');
            });
        }

        // PWA Install
        let deferredPrompt;
        window.addEventListener('beforeinstallprompt', (e) => {
            e.preventDefault();
            deferredPrompt = e;
            document.getElementById('install-button').classList.remove('hidden');
        });

        document.getElementById('install-button')?.addEventListener('click', async () => {
            if (deferredPrompt) {
                deferredPrompt.prompt();
                const { outcome } = await deferredPrompt.userChoice;
                deferredPrompt = null;
                document.getElementById('install-button').classList.add('hidden');
            }
        });

        // Register Service Worker
        if ('serviceWorker' in navigator) {
            navigator.serviceWorker.register('/sw.js');
        }

        // Initialize
        createParticles();
        setTimeout(() => {
            updateOverallProgress();
            updateExamCards();
        }, 500);
    </script>
</body>
</html>